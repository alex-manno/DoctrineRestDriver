{
  "name": "alex-manno/doctrine-rest-driver",
  "description": "Use a REST API as if it was your local database",
  "type": "library",
  "authors": [
    {
      "name": "Alessandro Manno",
      "email": "alessandro.manno@facile.it"
    }
  ],
  "keywords": [
    "REST API doctrine ORM DBAL driver"
  ],
  "license": [
    "GPL"
  ],
  "require-dev": {
    "facile/facile-coding-standard": "dev-master",
    "symfony/expression-language": "^3.0",
    "phpmd/phpmd" : "@stable",
    "phpunit/phpunit" : "@stable",
    "symfony/yaml": "3.*",
    "sensio/framework-extra-bundle": "3.*",
    "symfony/framework-bundle": "3.*",
    "doctrine/doctrine-bundle": "^1.6",
    "symfony/validator": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Manno\\DoctrineRestDriver\\": "src/"
    }
  },
  "require": {
    "php": ">=5.5",
    "doctrine/dbal": "2.*",
    "greenlion/php-sql-parser": "^4.0",
    "doctrine/orm": "2.*",
    "ci/restclientbundle": "^1.0"
  },
  "scripts": {
    "cs-check": "php-cs-fixer",
    "cs-fix": "php-cs-fixer fix"
  }
}
